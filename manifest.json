{
  "manifest_version": 2,
  "name": "DOM Destroyer",
  "description": "Remove stuff from websites like say an annoying sticky header, with two clicks.",
  "version" : "0.1",
  "browser_action": {
    "default_title": "Destroy DOM Nodes",
    "default_icon" : "destroy_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "homepage_url" : "http://github.com/uniphil/dom-destroyer"
}
